{
  "name": "gulp-fm-icon-converter",
  "version": "2.0.1",
  "description": "A gulp module to automatically add the fm_fill class to an svg icon",
  "main": "index.js",
  "scripts": {
    "start": "gulp --gulpfile example.js",
    "generate-readme": "mos",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "nyc _mocha --recursive ./tests --exit",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "keywords": [
    "svg",
    "FileMaker",
    "node",
    "icons",
    "gulp"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Luidog/gulp-fm-icon-converter.git"
  },
  "author": "Lui de la Parra",
  "license": "MIT",
  "peerDependencies": {
    "gulp": "^4.0.0"
  },
  "dependencies": {
    "gulp-cheerio": "^0.6.3",
    "snyk": "^1.123.0"
  },
  "mos": {
    "plugins": [
      "execute"
    ]
  },
  "resolutions": {
    "natives": "1.1.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.14.1",
    "eslint-config-google": "^0.12.0",
    "eslint-plugin-prettier": "^3.0.1",
    "gulp": "^4.0.0",
    "mocha": "^6.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mos": "^1.3.1",
    "mos-plugin-dependencies": "^2.2.2",
    "mos-plugin-execute": "^1.0.2",
    "mos-plugin-installation": "^1.2.2",
    "mos-plugin-license": "^1.2.2",
    "mos-plugin-snippet": "^2.1.3",
    "nyc": "^13.3.0",
    "prettier": "^1.16.4"
  },
  "snyk": true
}
